{
  "name": "HDPodBMKSDK",
  "module_name": "HDPodBMKSDK",
  "version": "0.0.1",
  "summary": "HDPodBMKSDK",
  "description": "这个是用到百度地图sdk的一个pod工程",
  "homepage": "https://github.com/erduoniba",
  "authors": {
    "denglibing": "13049862397@163.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "license": {
    "type": "MIT"
  },
  "source": {
    "git": "https://github.com/erduoniba/HDPodBMKSDK.git",
    "tag": "0.0.1"
  },
  "dependencies": {
    "BaiduMapKit": [

    ]
  },
  "frameworks": [
    "CoreLocation",
    "CoreTelephony",
    "OpenGLES",
    "QuartzCore",
    "Security",
    "SystemConfiguration",
    "BaiduMapAPI_Base",
    "BaiduMapAPI_Cloud",
    "BaiduMapAPI_Location",
    "BaiduMapAPI_Map",
    "BaiduMapAPI_Radar",
    "BaiduMapAPI_Search",
    "BaiduMapAPI_Utils"
  ],
  "libraries": [
    "z",
    "sqlite3.0",
    "stdc++.6.0.9",
    "crypto",
    "ssl"
  ],
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/BaiduMapKit/BaiduMapKit",
    "LIBRARY_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/BaiduMapKit/BaiduMapKit/thirdlibs",
    "OTHER_LDFLAGS": "$(inherited) -undefined dynamic_lookup -lObjC",
    "ENABLE_BITCODE": "NO"
  },
  "subspecs": [
    {
      "name": "HDPodBMKSDK",
      "source_files": "HDPodBMKSDK/*.{h,m,mm}"
    }
  ]
}
